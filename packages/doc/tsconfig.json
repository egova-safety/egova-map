{
    "extends": "../tsconfig.settings.json",
    "compilerOptions": {
        "module": "esnext",
        "target": "es5",
        "outDir": "dist",
        "baseUrl": "./",
        "paths": {
            "*": ["types/*"],
            "@egova/map-ui": [
                "../ui/src/*"
            ],
            "@/*": [
                "../ui/src/*"
            ],
            "@s/*": [
                "src/*"
            ]
        }
    },

    "references": [{
        "path": "../base"
    }, {
        "path": "../ui"
    }, {
        "path": "../arcgis"
    }, {
        "path": "../minemap"
    }]
}
